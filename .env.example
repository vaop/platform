APP_NAME=VAOP
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=example_app
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local-private
STORAGE_MODE=local
QUEUE_CONNECTION=database

# Embedded Queue Workers (enabled by default, disable for Docker)
# QUEUE_EMBEDDED_WORKERS=true
# QUEUE_WORKERS=2
# QUEUE_MAX_TIME=55
# QUEUE_MAX_JOBS=50
# QUEUE_MEMORY=128
# QUEUE_REST=1
# QUEUE_QUEUES=default

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# S3 Storage (when STORAGE_MODE=s3)
# Private bucket - for imports, exports, temp files
# S3_PRIVATE_ACCESS_KEY_ID=
# S3_PRIVATE_SECRET_ACCESS_KEY=
# S3_PRIVATE_DEFAULT_REGION=us-east-1
# S3_PRIVATE_BUCKET=
# S3_PRIVATE_ENDPOINT=
# S3_PRIVATE_URL=
# S3_PRIVATE_USE_PATH_STYLE_ENDPOINT=false

# Public bucket - for user uploads, avatars, public assets
# S3_PUBLIC_ACCESS_KEY_ID=
# S3_PUBLIC_SECRET_ACCESS_KEY=
# S3_PUBLIC_DEFAULT_REGION=us-east-1
# S3_PUBLIC_BUCKET=
# S3_PUBLIC_ENDPOINT=
# S3_PUBLIC_URL=
# S3_PUBLIC_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
